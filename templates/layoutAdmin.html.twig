    <script>
        $(document).ready(function(){
            $('#signup').click(function(){
                var email = $('#email_signup').val();
                var password = $('#password_signup').val();

                var fname = $('#fname').val();
                var lname = $('#lname').val();
                if($.trim(email).length > 0 )
                {
                    $.ajax({
                        url:"new-RH.php",
                        method:"POST",
                        data:{email:email, password:password  , fname:fname , lname:lname },
                        cache:false,
                        beforeSend:function(){
                            $('#signup').val("connecting...");
                        },
                        success:function(data)
                        {
                            if(data)
                            {
                                window.location.href="?a=RH";
                            }
                            else
                            {

                                $('#signup').val("Register");
                                $('#error3').html("<div class='text-center text-dark  alert alert-danger'>Email address is already registered</div>");

                            }
                        }
                    });
                }
                else
                {

                }
            });
        });
    </script>

           



